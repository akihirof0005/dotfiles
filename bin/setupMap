#!/bin/bash
chsh -s /bin/zsh
softwareupdate --install --all
/usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"
brew install git neovim
brew cask install google-chrome iterm2 mpv skype
brew update
brew upgrade
brew cask upgrade
softwareupdate --install --all
